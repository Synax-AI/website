  >
    <div class="container">
      <div class="row d-flex align-items-center">
        <!-- TEXT BLOCK -->
        <div class="col-md-5">
          <div class="txt-block left-column wow fadeInRight">
            <!-- Section ID -->

            <!-- Title -->
            <h2 class="s-46 w-700">Get in Touch</h2>

            <!-- Text -->
            <p>
